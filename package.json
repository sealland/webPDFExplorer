{
  "name": "pdf-explorer",
  "version": "1.0.0",
  "homepage": "/pdfviewer/",
  "description": "Web application for browsing and viewing PDF files in hierarchical folder structure",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-deps": "npm install --production",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "pdf",
    "explorer",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.1.10"
  }
}
